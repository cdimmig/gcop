<launch>	
	<arg name="parameters" default="$(find gcop_ctrl)/params/chain3.yaml"/>

	<include file="$(find gcop_urdf)/launch/display_chain.launch">
		<arg name="model" default="$(find gcop_ctrl)/params/chain3.urdf"/>
	</include>

  <rosparam command="load" file="$(arg parameters)" />
	<node name="mbs_sim" pkg="gcop_ctrl" type="mbssim" output="screen"/>
	<node name="mbsreconfigure" pkg="dynamic_reconfigure" type="reconfigure_gui" />
</launch>
